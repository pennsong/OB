@model IEnumerable<OB.Models.User>

@{
    ViewBag.Title = "CandidateList";
}

<h2>CandidateList</h2>

<p>
    @Html.ActionLink("Create New", "CreateCandidate")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }

</table>

@model OB.Models.Supplier

@{
    ViewBag.Name = "供应商";
    ViewBag.Title = "恢复";
}

<h1>@ViewBag.Name</h1>
<h2>@ViewBag.Title</h2>

<h3>您确定要恢复此条记录么?</h3>

<fieldset>
    <legend>@ViewBag.Name</legend>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.Name)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.IsPension)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsPension)
    </div>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.IsAccumulation)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsAccumulation)
    </div>
</fieldset>

<div>
    @using (Html.BeginForm("RestoreSave", "Supplier"))
    {
        @Html.AntiForgeryToken()
        @Html.Hidden("id", Model.Id)
        @Html.Hidden("returnUrl", @Url.Content((string)ViewBag.ReturnUrl))

        <input type="submit" value="确认恢复" />
    }
    <a href="@Url.Content((string)ViewBag.ReturnUrl)">取消</a>
</div>

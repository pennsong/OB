@model OB.Models.AccumulationType

@{
    ViewBag.Name = "公积金类型";
    ViewBag.Title = "详情";
}

<h1>@ViewBag.Name</h1>
<h2>@ViewBag.Title</h2>

<h3>您确定要删除此条记录么?</h3>

@Html.Action("Abstract", "AccumulationType", new { id = Model.Id })
<p>
    @using (Html.BeginForm("Edit", "AccumulationType"))
    {    
        @Html.AntiForgeryToken()

        @Html.Hidden("id", Model.Id)
        @Html.Hidden("returnUrl", @Url.Content((string)ViewBag.ReturnUrl))
        <input type="submit" value="编辑" />
    }
    <a href="@Url.Content((string)ViewBag.ReturnUrl)">返回</a>
</p>